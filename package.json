{
  "name": "suites-expose-reproduce",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^10.4.8",
    "@suites/di.nestjs": "^3.0.0",
    "@suites/doubles.jest": "^3.0.0",
    "@suites/unit": "^3.0.0",
    "@swc/cli": "^0.5.1",
    "@swc/core": "^1.9.3",
    "@swc/jest": "^0.2.37",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typescript": "^5.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s?$": [
        "@swc/jest"
      ]
    },
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "coverage/unit",
    "coverageReporters": [
      "html",
      "text-summary",
      "cobertura"
    ],
    "testEnvironment": "node"
  }
}
